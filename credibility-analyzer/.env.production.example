# Production Environment Variables for Render Deployment
# Copy these to Render's environment variables section

# API Configuration - Replace with your actual Render API URL
VITE_API_URL=https://credibility-analyzer-api.onrender.com

# Application Settings
VITE_API_TIMEOUT=30000
VITE_MAX_TEXT_LENGTH=10000
VITE_ITEMS_PER_PAGE=10
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false

# Sentry Error Tracking (optional)
VITE_SENTRY_ENABLED=false
VITE_SENTRY_DSN=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
